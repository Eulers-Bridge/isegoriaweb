<script>
      window.fbAsyncInit = function() {
        FB.init({
          appId      : '<%= FB_APP_ID%>',
          xfbml      : true,//Set to true if you have socia plugins loaded with xfbml
          //xfbml      : false,
          version    : 'v2.0'
        });
      };

      (function(d, s, id){
         var js, fjs = d.getElementsByTagName(s)[0];
         if (d.getElementById(id)) {return;}
         js = d.createElement(s); js.id = id;
         //js.src = "//connect.facebook.net/en_US/sdk.js";//minified version
         js.src = "//connect.facebook.net/en_US/sdk/debug.js";//debug version
         fjs.parentNode.insertBefore(js, fjs);
       }(document, 'script', 'facebook-jssdk'));
</script>